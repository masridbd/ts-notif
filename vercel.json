{
  "functions": {
    "api/message.js": {
      "memory": 1024,
      "maxDuration": 5
    },
    "api/telegram.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/message.txt",
      "dest": "/api/message"
    },
    {
      "src": "/telegram-webhook",
      "dest": "/api/telegram",
      "methods": ["POST"]
    }
  ]
}
